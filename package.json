{
  "name": "react-country-icon",
  "version": "1.0.0",
  "description": "React component to render country icon by country isoCode",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@material-ui/core": "^4.12.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikondrat/react-country-icon.git"
  },
  "keywords": [
    "react",
    "react",
    "library",
    "svg",
    "country",
    "flag"
  ],
  "author": "Kondrat Shmoylov <kondrat.shmoylov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ikondrat/react-country-icon/issues"
  },
  "homepage": "https://github.com/ikondrat/react-country-icon#readme",
  "scripts": {
    "build": "dotenv -c build -- webpack --config webpack.build.js",
    "prepare": "npm run lint && npm run test:ci && npm run build && npm run build:types",
    "version": "npm run prepare",
    "build:types": "tsc -p tsconfig.types.json",
    "start": "dotenv -c build -- webpack --config webpack.dev.js",
    "lint": "npm run lint:js",
    "lint:js": "eslint ./src --cache --cache-location .cache/eslint",
    "prettier:check": "npx prettier src --check",
    "prettier:write": "npx prettier src --write",
    "test": "jest --passWithNoTests --watch --colors",
    "test:ci": "npm run test -- --watchAll=false --ci",
    "test:integration:ci": "echo empty",
    "test:staged": "npm run test:ci -- --bail --findRelatedTests --coverage=false",
    "setup": "npm ci --legacy-peer-deps",
    "validate": "npm run lint && npm run test:ci",
    "precommit": "lint-staged"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "{src,cypress,tests}/**/*.{js,jsx,ts,tsx,json,css,scss,md,graphql}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "npm run test:staged"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.15.3",
    "@craco/craco": "^6.4.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.24",
    "@types/jest-axe": "^3.5.3",
    "@types/node": "^16.7.1",
    "@types/react": "17.0.15",
    "@types/react-dom": "17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@types/segment-analytics": "^0.0.34",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "bundle-loader": "^0.5.6",
    "classnames": "^2.3.1",
    "copy-webpack-plugin": "^9.0.1",
    "dotenv-cli": "^4.1.1",
    "eslint": "^7.32.0",
    "html-webpack-plugin": "^5.3.2",
    "jest-axe": "^5.0.1",
    "jest-sonar-reporter": "^2.0.0",
    "json": "^11.0.0",
    "lint-staged": "^12.4.1",
    "npm-add-script": "^1.1.0",
    "prettier": "^2.3.2",
    "react-scripts": "^4.0.3",
    "sass-loader": "^12.2.0",
    "ts-loader": "^9.1.2",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "4.3.5",
    "webpack": "^5.47.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "concurrently": "^7.0.0"
  }
}
